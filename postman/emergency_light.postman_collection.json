{
	"info": {
		"_postman_id": "<a-unique-id>",
		"name": "Emergency Lighting Detection",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Upload Blueprint",
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "formdata",
					"formdata": [
						{
							"key": "file",
							"type": "file",
							"src": "/path/to/your/blueprint.pdf"
						}
					]
				},
				"url": {
					"raw": "http://127.0.0.1:8000/blueprints/upload",
					"protocol": "http",
					"host": [
						"127",
						"0",
						"0",
						"1"
					],
					"port": "8000",
					"path": [
						"blueprints",
						"upload"
					]
				}
			},
			"response": []
		},
		{
			"name": "Get Result",
			"request": {
				"method": "GET",
				"header": [],
				"url": {
					"raw": "http://127.0.0.1:8000/blueprints/result?task_id=<your_task_id>",
					"protocol": "http",
					"host": [
						"127",
						"0",
						"0",
						"1"
					],
					"port": "8000",
					"path": [
						"blueprints",
						"result"
					],
					"query": [
						{
							"key": "task_id",
							"value": "<your_task_id>"
						}
					]
				}
			},
			"response": []
		}
	]
}